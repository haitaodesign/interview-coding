    "version": "0.2.0",
    "configurations": [
        {
            "name": "ts-node",
            "type": "node",
            "request": "launch",
            "args": [
                "./dist/index.js"
            ],
            "runtimeArgs": [
                "-r",
                "ts-node/register"
            ],
            "cwd": "${workspaceRoot}",
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Interview Coding JS",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "stopOnEntry": true,
            "program": "${workspaceFolder}/code-js/index.js"
        }
    ]
}